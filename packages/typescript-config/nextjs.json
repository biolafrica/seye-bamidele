{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "plugins": [{"name": "next"}],
    "module": "esnext",
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true
  }
}